<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:layout_margin="@dimen/_8sdp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.harati.jeevanbikas.Helper.CenturyGothicTextView
            android:padding="@dimen/_8sdp"
            android:textSize="@dimen/_13sdp"
            android:text="Base Url"
            android:maxLines="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.harati.jeevanbikas.Helper.CGEditText
            android:textSize="@dimen/_13sdp"
            android:maxLines="1"
            android:text="http://103.1.94.77:8087/agentbank"
            android:id="@+id/baseUrl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.harati.jeevanbikas.Helper.CGEditText>


    </LinearLayout>
    <LinearLayout
        android:layout_margin="@dimen/_8sdp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.harati.jeevanbikas.Helper.CenturyGothicTextView
            android:textSize="@dimen/_13sdp"
            android:padding="@dimen/_8sdp"
            android:text="Serial no"
            android:maxLines="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.harati.jeevanbikas.Helper.CGEditText
            android:textSize="@dimen/_13sdp"
            android:maxLines="1"
            android:text="101010"
            android:id="@+id/serialNo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.harati.jeevanbikas.Helper.CGEditText>


    </LinearLayout>
    <LinearLayout
        android:layout_margin="@dimen/_8sdp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.harati.jeevanbikas.Helper.CenturyGothicTextView
            android:textSize="@dimen/_13sdp"
            android:padding="@dimen/_8sdp"
            android:text="Username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.harati.jeevanbikas.Helper.CGEditText
            android:textSize="@dimen/_13sdp"
            android:maxLines="1"
            android:text="0004"
            android:id="@+id/api_username"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.harati.jeevanbikas.Helper.CGEditText>


    </LinearLayout>
    <LinearLayout
        android:layout_margin="@dimen/_8sdp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.harati.jeevanbikas.Helper.CenturyGothicTextView
            android:textSize="@dimen/_13sdp"
            android:padding="@dimen/_8sdp"
            android:text="Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.harati.jeevanbikas.Helper.CGEditText
            android:textSize="@dimen/_13sdp"
            android:text="1234"
            android:id="@+id/api_password"
            android:maxLines="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.harati.jeevanbikas.Helper.CGEditText>


    </LinearLayout>
    <LinearLayout
        android:layout_margin="@dimen/_8sdp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.harati.jeevanbikas.Helper.CenturyGothicTextView
            android:textSize="@dimen/_13sdp"
            android:padding="@dimen/_8sdp"
            android:text="Master Key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.harati.jeevanbikas.Helper.CGEditText
            android:textSize="@dimen/_13sdp"
            android:text="1234"
            android:id="@+id/api_master_key"
            android:maxLines="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.harati.jeevanbikas.Helper.CGEditText>


    </LinearLayout>


</LinearLayout>